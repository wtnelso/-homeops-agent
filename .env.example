# Application Configuration
VITE_APP_IS_LIVE=TRUE
VITE_APP_ENV=DEV
VITE_BYPASS_AUTH=FALSE
VITE_BETA_MODE=FALSE

# Supabase Configuration for Frontend
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Supabase Server Configuration  
SUPABASE_URL=your-supabase-project-url
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_ACCESS_TOKEN=your-supabase-access-token

# Google OAuth Configuration
VITE_GMAIL_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
VITE_GOOGLE_CALENDAR_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# OAuth Redirect URI Base
VITE_REDIRECT_URI_BASE=http://localhost:3000
REDIRECT_URI_BASE=http://localhost:3000

# Neon Database (for AI chat conversations and agent memory)
VITE_NEON_DATABASE_URL=postgresql://user:password@host/dbname

# OpenAI API (for LangChain AI agent)
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_OPENAI_MODEL=gpt-4
VITE_OPENAI_TEMPERATURE=0.7

# Optional: Alternative AI providers
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key

# Port (optional, defaults to 3000)
PORT=3000
