{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "devCommand": "npm run dev",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/embeddings/:path*",
      "destination": "https://homeops-agent-p7ru.onrender.com/api/embeddings/:path*"
    },
    {
      "source": "/health",
      "destination": "https://homeops-agent-p7ru.onrender.com/health"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}